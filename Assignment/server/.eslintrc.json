{
    "extends": "airbnb-base",

    "parserOptions": {
        "ecmaVersion": 6,
        "env": { "es6": true }
    },
    "rules": {
        "import/prefer-default-export": "off",
        "indent": ["error", 4],
        "no-console": "off",
        "linebreak-style": ["error", "windows"],
        "no-underscore-dangle": ["error", { "allow": ["_id"]}]
    }
}