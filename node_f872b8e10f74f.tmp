//-------- function to return today's date--------//

let todaysDate = function() {
    var date = new Date();

    var dd =  date.getDate();
    var mm = date.getMonth() + 1;
    var yyyy = date.getFullYear();
    
    

    return(dd+'/'+mm+'/'+yyyy)
}

console.log(todaysDate())

